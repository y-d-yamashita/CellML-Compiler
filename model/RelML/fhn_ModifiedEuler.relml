<relml>

	<!--    fhn Modified Euler relml version.1  	-->
	<!--	Last edit:   20130228  		-->
	<!--	Author:     m-ara			-->

	<tecml filename="model/TecML2.0/ModifiedEuler.tecml"/>
	<cellml name="cellml0" filename="model/CellML/fhn.cellml"/>
 
 	<tecmlvariable name="x1">
 		<correspondcellml file="cellml0" component="Main" name="x"/>
 		<correspondcellml file="cellml0" component="Main" name="y"/>
	</tecmlvariable>
	<tecmlvariable name="x2">
 		<correspondcellml file="cellml0" component="Main" name="x"/>
 		<correspondcellml file="cellml0" component="Main" name="y"/>
	</tecmlvariable>
	<tecmlvariable name="t1">
		<correspondcellml file="cellml0" component="Main" name="time" />
	</tecmlvariable>
	<tecmlvariable name="t2">
		<correspondcellml file="cellml0" component="Main" name="time" />
	</tecmlvariable>
	<tecmlvariable name="i1">
		<correspondcellml file="cellml0" component="Main" name="r" />
	</tecmlvariable>
	<tecmlvariable name="i2">
		<correspondcellml file="cellml0" component="Main" name="r" />
	</tecmlvariable>
	<tecmlvariable name="k1">
		<correspondcellml file="cellml0" component="Main" name="kx" />
		<correspondcellml file="cellml0" component="Main" name="ky" />
	</tecmlvariable>
	<tecmlvariable name="k2">
		<correspondcellml file="cellml0" component="Main" name="kx" />
		<correspondcellml file="cellml0" component="Main" name="ky" />
	</tecmlvariable>
	<tecmlvariable name="x1init">
		<correspondrelml value="-1.501250563778375" />
		<correspondrelml value="-0.37621367749846896" />
	</tecmlvariable>
	<tecmlvariable name="t1init">
		<correspondrelml value="0.0" />
	</tecmlvariable>
	<tecmlvariable name="z">
		<correspondcellml file="cellml0" component="Main" name="zz" />
		<correspondcellml file="cellml0" component="Main" name="beta" />
		<correspondcellml file="cellml0" component="Main" name="gamma" />
		<correspondcellml file="cellml0" component="Main" name="epsilon" />
	</tecmlvariable>
	<tecmlvariable name="d">
		<correspondrelml name="d" value="0.01"/>
	</tecmlvariable>
	<tecmlvariable name="x1end">
		<correspondrelml name="x"/>
		<correspondrelml name="y"/>
	</tecmlvariable>
	<tecmlvariable name="t1end">
		<correspondrelml name="t"/>
	</tecmlvariable>
	
	<math xmlns="http://www.w3.org/1998/Math/MathML">
		
	    <apply condname="cond" loopindex="n">
	        <lt/>
	        <apply>
		        <selector/>    
	        	<ci>Main_time_t1</ci>
	        	<ci>n</ci>
	        </apply>
	        <cn>400</cn>
	    </apply>
			
	
	</math>
	
</relml>