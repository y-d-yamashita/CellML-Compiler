<tecml>
	<!-- Euler tecml2.0 version.1  	-->
	<!--	Last edit:   20121008  	-->
	<!--	Author:       m-ara		-->

	<variable name="x" type="recurvar" />
	<variable name="t" type="recurvar" />

	<variable name="i" type="arithvar" />
	<variable name="k" type="arithvar" />

	<variable name="xinit" type="constvar" />
	<variable name="tinit" type="constvar" />
	<variable name="z" type="constvar" />
	<variable name="d" type="constvar" />
	
	<variable name="cond" type="condition" />

	<variable name="xend" type="arithvar" />
	<variable name="tend" type="arithvar" />
	
	<variable name="n" type="stepvar" />

	<function name="g" />
	<function name="f" />

	<math xmlns="http://www.w3.org/1998/Math/MathML">
		<apply type="init" loopindex="n">
			<eq/>
			<apply>
				<selector/>
				<ci>x</ci>
				<apply>
					<eq/>
					<ci>n</ci>
					<cn>0</cn>
				</apply>
			</apply>
			<ci>xinit</ci>
		</apply>

		<apply type="init" loopindex="n">
			<eq/>
			<apply>
				<selector/>
				<ci>t</ci>
				<apply>
					<eq/>
					<ci>n</ci>
					<cn>0</cn>
				</apply>
			</apply>
			<ci>tinit</ci>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>i</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<apply>
					<selector/>
					<ci>x</ci>
					<apply>
						<plus/>
						<ci>n</ci>
						<cn>1</cn>
					</apply>
				</apply>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>i</ci>
					<ci>n</ci>
				</apply>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>k</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<apply>
					<selector/>
					<ci>x</ci>
					<apply>
						<plus/>
						<ci>n</ci>
						<cn>1</cn>
					</apply>
				</apply>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>i</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>k</ci>
					<ci>n</ci>
				</apply>
				<ci>z</ci>
			</apply>
		</apply>
		
		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>x</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>x</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<times/>
					<apply>
						<selector/>
						<ci>k</ci>
						<ci>n</ci>
					</apply>
					<ci>d</ci>
				</apply>
			</apply>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>t</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<ci>d</ci>
			</apply>
		</apply>
		
		<apply type="final" loopindex="n">
			<condition>
				<ci>cond</ci>
			</condition>	
			<eq/>
			<ci>xend</ci>
			<apply>
				<selector/>
				<ci>x</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
		</apply>
		

		<apply type="final" loopindex="n">
			<condition>
				<ci>cond</ci>
			</condition>	
			<eq/>
			<ci>tend</ci>
			<apply>
				<selector/>
				<ci>t</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
		</apply>
		
	</math>

</tecml>