<tecml>
	<!--    Implicit Runge-Kutta tecml2.0 version.1  	-->
	<!--	Last edit:   20130122  		-->
	<!--	Author:     m-ara			-->

	<variable name="x" type="recurvar" />
	<variable name="x1" type="recurvar" />
	<variable name="x2" type="recurvar" />
	<variable name="t" type="recurvar" />
	<variable name="t1" type="recurvar" />
	<variable name="t2" type="recurvar" />

	<variable name="i1" type="arithvar" />
	<variable name="i2" type="arithvar" />
	<variable name="k1" type="arithvar" />
	<variable name="k2" type="arithvar" />

	<variable name="xinit" type="constvar" />
	<variable name="tinit" type="constvar" />
	<variable name="z" type="constvar" />
	<variable name="d" type="constvar" />
	
	<variable name="cond" type="condition" />

	<variable name="xend" type="arithvar" />
	<variable name="tend" type="arithvar" />

	<function name="g" />
	<function name="f" />
	
	<variable name="n" type="stepvar" />

	<math xmlns="http://www.w3.org/1998/Math/MathML">
		<apply type="init" loopindex="n">
			<eq/>
			<apply>
				<selector/>
				<ci>x</ci>
				<apply>
					<eq/>
					<ci>n</ci>
					<cn>0</cn>
				</apply>
			</apply>
			<ci>xinit</ci>
		</apply>

		<apply type="init" loopindex="n">
			<eq/>
			<apply>
				<selector/>
				<ci>t</ci>
				<apply>
					<eq/>
					<ci>n</ci>
					<cn>0</cn>
				</apply>
			</apply>
			<ci>tinit</ci>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>x1</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>x</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<times/>
					<apply>
						<selector/>
						<ci>k1</ci>
						<ci>n</ci>
					</apply>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>4</cn>
					</apply>
				</apply>
				<apply>
					<times/>
					<apply>
						<selector/>
						<ci>k2</ci>
						<ci>n</ci>
					</apply>
					<apply>
						<times/>
						<ci>d</ci>
						<apply>
							<minus/>
							<apply>
								<divide/>
								<cn>1</cn>
								<cn>4</cn>
							</apply>
							<apply>
								<divide/>
								<apply>
									<root/>
									<cn>3</cn>
								</apply>
								<cn>6</cn>
							</apply>
						</apply>
					</apply>
				</apply>
			</apply>
		</apply>
		
		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>t1</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<times/>
					<ci>d</ci>
					<apply>
						<minus/>
						<apply>
							<divide/>
							<cn>1</cn>
							<cn>2</cn>
						</apply>
						<apply>
							<divide/>
							<apply>
								<root/>
								<cn>3</cn>
							</apply>
							<cn>6</cn>
						</apply>
					</apply>
				</apply>
			</apply>
		</apply>						

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>i1</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<apply>
					<selector/>
					<ci>x1</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>i1</ci>
					<ci>n</ci>
				</apply>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>k1</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<apply>
					<selector/>
					<ci>x1</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>t1</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>i1</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>k1</ci>
					<ci>n</ci>
				</apply>
				<ci>z</ci>
			</apply>
		</apply>
		
		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>x2</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>x</ci>
					<cn>n</cn>
				</apply>
				<apply>
					<times/>
					<apply>
						<selector/>
						<ci>k1</ci>
						<ci>n</ci>
					</apply>
					<apply>
						<times/>
						<ci>d</ci>
						<apply>
							<plus/>
							<apply>
								<divide/>
								<cn>1</cn>
								<cn>4</cn>
							</apply>
							<apply>
								<divide/>
								<apply>
									<root/>
									<cn>3</cn>
								</apply>
								<cn>6</cn>
							</apply>
						</apply>
					</apply>
				</apply>
				<apply>
					<times/>
					<apply>
						<selector/>
						<ci>k2</ci>
						<ci>n</ci>
					</apply>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>4</cn>
					</apply>
				</apply>
			</apply>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>t2</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<times/>
					<ci>d</ci>
					<apply>
						<plus/>
						<apply>
							<divide/>
							<cn>1</cn>
							<cn>2</cn>
						</apply>
						<apply>
							<divide/>
							<apply>
								<root/>
								<cn>3</cn>
							</apply>
							<cn>6</cn>
						</apply>
					</apply>
				</apply>
			</apply>
		</apply>
		
		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>i2</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<apply>
					<selector/>
					<ci>x2</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>i2</ci>
					<ci>n</ci>
				</apply>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>k2</ci>
				<ci>n</ci>
			</apply>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<apply>
					<selector/>
					<ci>x2</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>t2</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>i2</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<selector/>
					<ci>k2</ci>
					<ci>n</ci>
				</apply>
				<ci>z</ci>
			</apply>
		</apply>
		
		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>x</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>x</ci>
					<ci>n</ci>
				</apply>
				<apply>
					<times/>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
					<apply>
						<plus/>
						<apply>
							<selector/>
							<ci>k1</ci>
							<ci>n</ci>
						</apply>
						<apply>
							<selector/>
							<ci>k2</ci>
							<ci>n</ci>
						</apply>
					</apply>
				</apply>
			</apply>
		</apply>

		<apply>
			<eq/>
			<apply>
				<selector/>
				<ci>t</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>t</ci>
					<ci>n</ci>
				</apply>
				<ci>d</ci>
			</apply>
		</apply>
		
		<apply type="final" loopindex="n">
			<condition>
				<apply>
					<ci>cond</ci>
				</apply>
			</condition>	
			<eq/>
			<ci>xend</ci>
			<apply>
				<selector/>
				<ci>x</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
		</apply>

		<apply type="final" loopindex="n">
			<condition>
				<apply>
					<ci>cond</ci>
				</apply>
			</condition>	
			<eq/>
			<ci>tend</ci>
			<apply>
				<selector/>
				<ci>t</ci>
				<apply>
					<plus/>
					<ci>n</ci>
					<cn>1</cn>
				</apply>
			</apply>
		</apply>
		
	</math>

</tecml>