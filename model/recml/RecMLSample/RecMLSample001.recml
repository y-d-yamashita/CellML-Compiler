<recml>
<!-- single loop -->
	<!-- loop1 pre -->
	<!-- loop1 init -->
	<!-- loop1 inner -->
	<!-- loop1 loopcond -->
	<!-- loop1 final -->
	<!-- loop1 post -->

<!-- loop1 ������ -->
	<loopstruct num="0">
		<position name="inner">
		</position>
	</loopstruct>

	<loopindex num="0" name="n1" />
	
	<variable name="X1" type="recurvar" loopcomponent1="1" />
	<variable name="kX1" type="arithvar" loopcomponent1="1" />
	<variable name="X1end" type="output" loopcomponent1="1" />

	<variable name="t1" type="recurvar" loopcomponent1="1" />
	<variable name="t1end" type="output" loopcomponent1="1" />

	<variable name="Y1" type="arithvar" loopcomponent1="1" />

<!--	<variable name="n1" type="stepvar" loopcomponent1="1" /> -->
	
	<variable name="delt1" type="constvar" loopcomponent1="1" />

	

	<math>
		<!-- loop1 pre -->
		
		<!-- loop1 init -->
		<apply loop1 = "init">
			<eq/>
			<apply>
				<selector/>
				<ci>X1</ci>
				<cn>0</cn>
			</apply>
			<ci>X1init</ci>
		</apply>
	
		<apply loop1 = "init">
			<eq/>
			<apply>
				<selector/>
				<ci>t1</ci>
				<cn>0</cn>
			</apply>
			<ci>t1init</ci>
		</apply>

		<!-- loop1 inner -->
		<apply loop1 = "inner">
			<eq/>
			<apply>
				<selector/>
				<ci>Y1</ci>
				<ci>n1</ci>
			</apply>
			<apply>
				<apply>
					<times/>
					<apply>
						<minus/>
						<apply>
							<cn>1</cn>
						</apply>
					</apply>	
					<apply>
						<selector/>
						<ci>X1</ci>
						<ci>n1</ci>
					</apply>
				</apply>
			</apply>
		</apply>

		<apply loop1 = "inner">
			<eq/>
			<apply>
				<selector/>
				<ci>kX1</ci>
				<ci>n1</ci>
			</apply>
			<apply>
				<selector/>
				<ci>Y1</ci>
				<ci>n1</ci>
			</apply>
		</apply>
		
		<apply loop1 = "inner">
			<eq/>
			<apply>
				<selector/>
				<ci>X1</ci>
				<apply>
					<plus/>
					<ci>n1</ci>
					<cn>1</cn>
				</apply>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>X1</ci>
					<ci>n1</ci>
				</apply>
				<apply>
					<times/>
					<apply>
						<selector/>
						<ci>kX1</ci>
						<ci>n1</ci>
					</apply>
					<ci>delt1</ci>
				</apply>
			</apply>
		</apply>
		
		<apply loop1 = "inner">
			<eq/>
			<apply>
				<selector/>
				<ci>t1</ci>
				<apply>
					<plus/>
					<ci>n1</ci>
					<cn>1</cn>
				</apply>
			</apply>
			<apply>
				<plus/>
				<apply>
					<selector/>
					<ci>t1</ci>
					<ci>n1</ci>
				</apply>
				<ci>delt1</ci>
			</apply>
		</apply>
			
		<!-- loop1 loopcond -->
		<condition loop1 = "loopcond">
			<apply>
				<eq/>
				<ci>n1</ci>
				<cn>100</cn>
			</apply>
		</condition>
		
		<!-- loop1 final -->
		<apply loop1 = "final">
			<eq/>
			<ci>X1end</ci>
			<apply>
				<selector/>
				<ci>X1</ci>
				<ci>n1</ci>
			</apply>
		</apply>
		
		<apply loop1 = "final">
			<eq/>
			<ci>t1end</ci>
			<apply>
				<selector/>
				<ci>t1</ci>
				<ci>n1</ci>
			</apply>
		</apply>
		
		<!-- loop1 post -->		
	</math>
</recml>