<!-- double loop -->
<!-- loop1="inner" の中に loop2 がある -->
<recml>
  <loopindex num="0" name="tn" />
  <loopindex num="1" name="tm" />
  
  <math>

    <apply loop1 = "pre">
      <eq/>
      <ci>a</ci>
      <ci>b</ci>
    </apply>

    <apply loop1 = "init">
      <eq/>
      <ci>x</ci>
      <ci>y</ci>
    </apply>

    <apply loop1 = "inner" loop2 = "pre">
      <eq/>
      <ci>p</ci>
      <ci>q</ci>
    </apply>

    <apply loop1 = "inner" loop2 = "init">
      <eq/>
      <ci>r</ci>
      <ci>s</ci>
    </apply>

    <apply loop1 = "inner" loop2="inner">
      <eq/>
		<apply>
		  <selector/>
		  <ci>r</ci>
		  <ci>tm</ci>
		</apply>
		<apply>
		  <cn>2</cn>
		</apply>
    </apply>
    
    <condition loop1 = "inner" loop2="loopcond">
      <apply>
        <eq/>
        <ci>tm</ci>
        <cn>1000</cn>
      </apply>
    </condition>
    
    <apply loop1 = "inner" loop2="final">
      <eq/>
      <apply>
        <selector/>
        <ci>r</ci>
        <ci>tm</ci>
      </apply>
      <cn>100</cn>
    </apply>
    
    <apply loop1 = "inner" loop2="post">
      <eq/>
      <ci>x</ci>
      <cn>0</cn>
    </apply>

    <condition loop1="loopcond">
      <apply>
        <eq/>
        <ci>tn</ci>
        <cn>1000</cn>
      </apply>
    </condition>
    
    <apply loop1="final">
      <eq/>
      <apply>
        <selector/>
        <ci>x</ci>
        <ci>tn</ci>
      </apply>
      <cn>100</cn>
    </apply>
    
    <apply loop1="post">
      <eq/>
      <ci>y</ci>
      <cn>0</cn>
    </apply>

  </math>
</recml>
  