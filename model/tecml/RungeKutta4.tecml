<tecml>
	<!-- 4th Order Runge-Kutta version 1b  	-->
	<!--	Last edit:   20110726   	-->
	<!--	Author:      fr-punzalan  	-->

	<inputvar name="xi" type="diffvar" />
	<outputvar name="xo" type="diffvar" />

	<variable name="d" type="deltatimevar" />

	<variable name="t" type="timevar" />

	<variable name="k1" type="derivativevar" />
	<variable name="k2" type="derivativevar" />
	<variable name="k3" type="derivativevar" />
	<variable name="k4" type="derivativevar" />

	<variable name="x0" type="diffvar" />
	<variable name="x1" type="diffvar" />
	<variable name="x2" type="diffvar" />
	<variable name="x3" type="diffvar" />
	<variable name="x4" type="diffvar" />

	<variable name="y0" type="arithvar" />
	<variable name="y1" type="arithvar" />
	<variable name="y2" type="arithvar" />
	<variable name="y3" type="arithvar" />

	<variable name="z" type="constvar" />

	<function name="g" type="nondiffequ">
		 <argument type="diffvar" />
		 <argument type="timevar" />
		 <argument type="arithvar" />
		 <argument type="constvar" />
	</function>

	<function name="f" type="diffequ">
		 <argument type="diffvar" />
		 <argument type="timevar" />
		 <argument type="arithvar" />
		 <argument type="constvar" />
	</function>


	<math xmlns="http://www.w3.org/1998/Math/MathML">
		<apply>
			<eq/>
			<ci>x0</ci>
			<ci>xi</ci>
		</apply>

		<apply>
			<eq/>
			<ci>y0</ci>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<ci>x0</ci>
				<ci>t</ci>
				<ci>y0</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>k1</ci>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<ci>x0</ci>
				<ci>t</ci>
				<ci>y0</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>x1</ci>
			<apply>
				<plus/>
				<ci>x0</ci>
				<apply>
					<times/>
					<ci>k1</ci>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
				</apply>
			</apply>
		</apply>


		<apply>
			<eq/>
			<ci>y1</ci>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<ci>x1</ci>
				<apply>
        				<plus/>
        				<ci>t</ci>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
      				</apply>
				<ci>y1</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>k2</ci>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<ci>x1</ci>
				<apply>
        				<plus/>
        				<ci>t</ci>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
      				</apply>
				<ci>y1</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>x2</ci>
			<apply>
				<plus/>
				<ci>x0</ci>
				<apply>
					<times/>
					<ci>k2</ci>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
				</apply>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>y2</ci>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<ci>x2</ci>
				<apply>
        				<plus/>
        				<ci>t</ci>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
      				</apply>
				<ci>y2</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>k3</ci>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<ci>x2</ci>
				<apply>
        				<plus/>
        				<ci>t</ci>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>2</cn>
					</apply>
      				</apply>
				<ci>y2</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>x3</ci>
			<apply>
				<plus/>
				<ci>x0</ci>
				<apply>
					<times/>
					<ci>k3</ci>
					<ci>d</ci>
				</apply>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>y3</ci>
			<apply>
				<fn>
					<ci>g</ci>
				</fn>
				<ci>x3</ci>
				<apply>
        				<plus/>
        				<ci>t</ci>
					<ci>d</ci>
      				</apply>
				<ci>y3</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>k4</ci>
			<apply>
				<fn>
					<ci>f</ci>
				</fn>
				<ci>x3</ci>
				<apply>
        				<plus/>
        				<ci>t</ci>
					<ci>d</ci>
      				</apply>
				<ci>y3</ci>
				<ci>z</ci>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>x4</ci>
			<apply>
				<plus/>
				<ci>x0</ci>
				<apply>
					<times/>
					<apply>
						<divide/>
						<ci>d</ci>
						<cn>6</cn>
					</apply>
					<apply>
						<plus/>
						<ci>k1</ci>
						<apply>
							<times/>
							<cn>2</cn>
							<ci>k2</ci>
						</apply>
						<apply>
							<times/>
							<cn>2</cn>
							<ci>k3</ci>
						</apply>
						<ci>k4</ci>
					</apply>
				</apply>
			</apply>
		</apply>

		<apply>
			<eq/>
			<ci>xo</ci>
			<ci>x4</ci>
		</apply>

	</math>

</tecml>