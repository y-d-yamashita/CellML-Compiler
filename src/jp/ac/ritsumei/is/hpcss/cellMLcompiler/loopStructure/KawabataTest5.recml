<recml>
	<loopindex num="0" name="n1" />

	<node id="0" equation="0" variable = "0" />
	<node id="1" equation="1" variable = "1" />
	<node id="2" equation="2" variable = "2" />
	<node id="3" equation="3" variable = "3" />
	
	<edge source = "0" dest = "1" />
	<edge source = "0" dest = "2" />
	<edge source = "0" dest = "3" />
	<edge source = "1" dest = "2" />
	<edge source = "1" dest = "3" />
	<edge source = "2" dest = "3" />
	
	<math>

		<apply num = "0">
			<eq/>
			<ci>A</ci>
			<cn>0</cn>
		</apply>

		<apply num = "1" type = "initend" loopnum = "0">
			<eq/>
			<ci>B</ci>
			<cn>A</cn>
		</apply>

		<apply num = "2">
			<eq/>
			<ci>C</ci>
			<apply>
			<plus/>
				<ci>A</ci>
				<ci>B</ci>
			</apply>
		</apply>
		
		<apply num = "3" type = "final" loopnum = "0">
			<eq/>
			<ci>D</ci>
			<apply>
			<plus/>
				<ci>A</ci>
				<ci>B</ci>
				<ci>C</ci>
			</apply>
		</apply>

	</math>

</recml>